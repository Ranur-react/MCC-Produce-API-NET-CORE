
@{
    Layout = "_LayoutSBLogin";
}

<!-- Main Content -->
<div id="content">

    <!-- Begin Page Content -->
    <div class="container-fluid">

        <!-- 404 Error Text -->
        <div class="text-center">
            <div class="error mx-auto" data-text="404">404</div>
            <p class="lead text-gray-800 mb-5">Page Not Found</p>
            <p class="text-gray-500 mb-0">Your destinations page it's not available on the system Contact administrator to ask rahmat.nur@ms.mii.com </p>
            <a href="#" onclick="GoLogin('@ViewData["returnUrl"]');">&rarr; Back</a>
        </div>

    </div>
    <!-- /.container-fluid -->
</div>
<script>
    window.onload = () => {
    var time = 5;
        console.log("Start . .");
        var timer=setInterval(() => {
            time--;
            console.log(time);
            if (time < 1) {
                clearInterval(timer);
                GoLogin('@ViewData["returnUrl"]');
            }
        },1000);

    }
    const GoLogin = (returnUrl) => {
       history.back()
    }
</script>
<!-- End of Main Content -->
